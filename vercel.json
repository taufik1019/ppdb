{
    "version": 2,
    "builds": [
        { "src": "/**", "use": "@vercel/static" },
        { "src": "index.php", "use": "vercel-php@0.6.0" }
    ],
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "index.php"
        }
    ]
}